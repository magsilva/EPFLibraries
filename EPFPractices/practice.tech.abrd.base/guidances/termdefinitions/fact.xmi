<org.eclipse.epf.uma:ContentDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.eclipse.epf.uma="http://www.eclipse.org/epf/uma/1.0.5/uma.ecore" xmlns:epf="http://www.eclipse.org/epf" epf:version="1.5.1" xmi:id="-BoxqXrLpUsIO-6SMZOTKlQ" name="new_term_definition_2,_B6TA0BtrEdu_BZ3CL1I6hw" guid="-BoxqXrLpUsIO-6SMZOTKlQ" authors="Jerome Boyer" changeDate="2008-09-03T19:59:07.432-0300" version="1.0.0">
  <mainDescription>&lt;a id=&quot;XE_fact&quot; name=&quot;XE_fact&quot;>&lt;/a> &#xD;
&lt;p class=&quot;MsoNormal&quot; style=&quot;MARGIN: 3pt 0in; tab-stops: list .5in 1.0in&quot;>&#xD;
    &lt;span style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial&quot;>&lt;span style=&quot;mso-bidi-language: HE&quot;>Facts are combinations of terms&#xD;
    that describe what business people know about their business. It&lt;/span> &lt;span&#xD;
    style=&quot;mso-bidi-font-size: 10.0pt; mso-bidi-font-family: Arial&quot;>connects terms into sensible business relevant&#xD;
    observations.&lt;/span> &lt;span style=&quot;mso-bidi-language: HE&quot;>Facts may describe the relationships between terms, like an&#xD;
    Insurance Policy is a form of Contract, or it may describe the interactions between terms&#xD;
    (collaboration).&lt;/span>&lt;/span>&#xD;
&lt;/p>&#xD;
&lt;p class=&quot;MsoNormal&quot; style=&quot;MARGIN: 3pt 0in; tab-stops: list .5in 1.0in&quot;>&#xD;
    &lt;span style=&quot;mso-bidi-language: HE&quot;>Here are some examples of facts&lt;/span>&#xD;
&lt;/p>&#xD;
&lt;ul>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 3pt 0in; tab-stops: list .5in 1.0in&quot;>&#xD;
            &lt;span style=&quot;mso-bidi-language: HE&quot;>A coverage is the amount of protection against loss.&lt;/span>&#xD;
        &lt;/div>&#xD;
    &lt;/li>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 3pt 0in; tab-stops: list .5in 1.0in&quot;>&#xD;
            &lt;span style=&quot;mso-bidi-language: HE&quot;>A Deductible is the amount the Insured must pay when a loss occurs.&lt;/span>&#xD;
        &lt;/div>&#xD;
    &lt;/li>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 3pt 0in; tab-stops: list .5in 1.0in&quot;>&#xD;
            &lt;span style=&quot;mso-bidi-language: HE&quot;>A user must be part of a group, and the group includes the set of&#xD;
            permissions the user can use.&lt;/span>&#xD;
        &lt;/div>&#xD;
    &lt;/li>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 0in 0in 0pt&quot;>&#xD;
            &lt;span style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial&quot;>&lt;span style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial&quot;>A taxpayer&#xD;
            files a tax return form.&lt;/span>&lt;/span>&#xD;
        &lt;/div>&#xD;
    &lt;/li>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 0in 0in 0pt&quot;>&#xD;
            &lt;span style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial&quot;>A insured enters a claim in the system.&lt;/span>&#xD;
        &lt;/div>&#xD;
    &lt;/li>&#xD;
    &lt;li>&#xD;
        &lt;div class=&quot;MsoNormal&quot; style=&quot;MARGIN: 0in 0in 0pt&quot;>&#xD;
            &lt;span&#xD;
            style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial; mso-bidi-language: AR-SA; mso-fareast-font-family: 'Times New Roman'; mso-ansi-language: EN-US; mso-fareast-language: EN-US&quot;>&lt;span&#xD;
             style=&quot;FONT-SIZE: 10pt; FONT-FAMILY: Arial; mso-bidi-language: AR-SA; mso-fareast-font-family: 'Times New Roman'; mso-ansi-language: EN-US; mso-fareast-language: EN-US&quot;>A&#xD;
            customer could have only one purchase order at a time&lt;/span>&lt;/span>&#xD;
        &lt;/div>&lt;br />&#xD;
    &lt;/li>&#xD;
&lt;/ul></mainDescription>
